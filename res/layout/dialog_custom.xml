                    android:textSize="10sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/history_status_dialog"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:textSize="12sp" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dip"
                android:background="#ffffff" >

                <TextView
                    android:id="@+id/history_uploaded_title_dialog"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:padding="5dip"
                    android:text="@string/uploaded_title_dialog"
                    android:textSize="12sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/history_uploaded_title2_dialog"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:text="@string/doubleDot"
                    android:textSize="10sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/history_uploaded_dialog"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:padding="10dip"
                    android:textSize="12sp" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dip"
                android:background="#ffffff" >

                <TextView
                    android:id="@+id/history_commentofficer_title_dialog"
                    android:layout_width="20dp"
                    android:layout_height="wrap_content"
                    android:padding="5dip"
                    android:text="@string/commentofficer_title_dialog"
                    android:textSize="12sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/history_commentofficer_title2_dialog"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:text="@string/doubleDot"
                    android:textSize="12sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/history_commentofficer_dialog"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:padding="10dip"
                    android:textSize="12sp" />
            </TableRow>
   
        </TableLayout>
    </ScrollView>

</RelativeLayout>